{
	"name": "bharti-dark",
	"colors": {
		// terminal panel 
		"panelTitle.activeForeground": "#00eeff",
		"panelTitle.inactiveForeground": "#2a9d8f",
		"panel.border": "#2a9d8f",
		"panel.background": "#15141d",
		//status bar
		"statusBar.background": "#000000",
		"statusBar.foreground": "#2a9d8f",
		//input goes here 
		"input.border": "#02b7ff",
		"input.background": "#050505",
		"input.foreground": "#ffffff",
		//side bar
		"sideBar.foreground": "#2a9d8f",
		"sideBar.background": "#000000",
		"sideBarTitle.foreground": "#2a9d8f",
		//activitybar 
		"activityBar.background": "#000000",
		"activityBarBadge.foreground": "#1d2433",
		"activityBarBadge.background": "#ffcc66",
		"activityBar.foreground": "#2a9d8f",
		//button 
		"button.background": "#02b7ff",
		"button.foreground": "#171c28",
		"button.hoverBackground": "#fc7a01",
		// whole window
		"window.activeBorder": "#00eeff",
		//badge
		"badge.foreground": "#ff0101",
		"badge.background": "#ECB365",
		//errors and warning 
		"editorError.foreground": "#ef6b73",
		"editorWarning.foreground": "#ffd580",
		"editorInfo.foreground": "#5ccfe6",
		"editorHint.foreground": "#f4a261",
		//editor goes here 
		"editor.background": "#0a192f",
		"editor.foreground": "#64ff9f",
		"editorLineNumber.foreground": "#2a9d8f",
		"editorCursor.foreground": "#00eeff",
		"editor.findMatchHighlightBackground": "#3e0266",
		"editorGroupHeader.tabsBackground": "#000000",
		"editorGroup.border": "#f4acb7",
		"editorGroup.dropBackground": "#0cb8e84d",
		//tabs 
		"tab.activeBackground": "#000000",
		"tab.activeBorderTop": "#2a9d8f",
		"tab.inactiveBackground": "#000000",
		"tab.activeForeground": "#2a9d8f",
		"tab.hoverBackground": "#0000",
		//terminal  
		"terminal.background": "#0a192f",
		"terminal.foreground": "#335c67",
		// dropdown
		"dropdown.background": "#000000",
		"dropdown.foreground": "#00eeff",
		"dropdown.border": "#00eeff",
		"dropdown.listBackground": "#0d0727",
		// title-bar 
		"titleBar.activeBackground": "#000000",
		// "titleBar.border": "#2a9d8f",
		"titleBar.activeForeground": "#2a9d8f",
		// menubar goes here 
		"menubar.selectionForeground": "#00eeff",
		"menu.background": "#22223b",
		"menu.foreground": "#00eeff",
		"menu.border": "#00eeff",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#19475c"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#24e7e7"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#af47b3"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#ec0d32"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#a47ac0"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#52c5f3"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#b1565b"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#617ebd"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#521c1f"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#c9242c"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#441968"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#c5b34f"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#816126"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#1e8fbb"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
